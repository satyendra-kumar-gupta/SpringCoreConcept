<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
   <!-- this is beans-->
    <!-- value as a element-->
    <bean class="org.saty.Student" name="student">
     <property name="id">
         <value>111</value>
     </property>
        <property name="name">
            <value>Satyendra</value>
        </property>
        <property name="address">
            <value>Uttar Pradesh</value>
        </property>
    </bean>

    <!-- Values as attribute-->
    <bean class="org.saty.Student" name="student1">
        <property name="id" value="222"/>
        <property name="name" value="XYZ"/>
        <property name="address" value="ABC"/>
    </bean>

   <!-- values use as p schema -->
  <!--  <bean class="org.saty.Student" name="student2" p:id="333" p:name="Hello" p:address="hello"/> -->
</beans>