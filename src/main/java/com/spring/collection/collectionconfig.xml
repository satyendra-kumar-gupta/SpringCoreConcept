<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="emp" class="com.spring.collection.Emp">
        <property name="name" value="Radhe" />
        <property name="phones">
            <list>
                <value>1234567890</value>
                <value>0987654321</value>
                <value>1236547890</value>
                <value>1237896540</value>
               <null/>
            </list>
        </property>
        <property name="address">
            <set>
                <value>Bnagalore</value>
                <value>Hyderabad</value>
                <value>Delhi</value>
                <value>Locknow</value>
            </set>
        </property>

        <property name="courses">
            <map>
                <entry key="java" value="2month" />
                <entry key="python" value="1month" />
                <entry key="js" value="1month" />
                <entry key="html" value="1month" />
            </map>
        </property>
    </bean>



</beans>